{
  "name": "template-vue",
  "version": "1.0.0",
  "author": "katashin",
  "private": true,
  "description": "A project template for a Vue.js application",
  "license": "MIT",
  "homepage": "https://github.com/ktsn/template-vue",
  "bugs": "https://github.com/ktsn/template-vue/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/template-vue.git"
  },
  "scripts": {
    "clean": "rm -rf .tmp dist",
    "build": "webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:test": "webpack --config webpack.config.test.js",
    "watch": "webpack -w --config webpack.config.js",
    "watch:test": "webpack -w --config webpack.config.test.js",
    "lint": "eslint --fix 'src/**/*.+(js|vue)' && eslint --fix 'test/**/*.js'",
    "minify": "uglifyjs dist/main.js -c -m -o dist/main.js",
    "testem": "testem",
    "testem:ci": "testem ci --launch Firefox,PhantomJS",
    "test": "run-p watch:test testem",
    "test:ci": "run-s lint build:test testem:ci"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "eslint-plugin-html": "^1.5.2",
    "glob": "^7.0.3",
    "json-loader": "^0.5.4",
    "power-assert": "^1.3.1",
    "testem": "^1.6.0",
    "uglify-js": "^2.7.0",
    "vue-loader": "^9.3.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "vue": "^2.0.0-beta.7",
    "vue-router": "^2.0.0-rc.2",
    "vuex": "^2.0.0-rc.4",
    "vuex-router-sync": "^3.0.0"
  }
}
