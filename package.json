{
  "name": "template-vue",
  "version": "1.0.0",
  "author": "katashin",
  "private": true,
  "description": "A project template for a Vue.js application",
  "license": "MIT",
  "homepage": "https://github.com/ktsn/template-vue",
  "bugs": "https://github.com/ktsn/template-vue/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/template-vue.git"
  },
  "scripts": {
    "clean": "rm -rf .tmp dist",
    "build": "webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:test": "webpack --config webpack.config.test.js",
    "watch": "webpack -w --config webpack.config.js",
    "watch:test": "webpack -w --config webpack.config.test.js",
    "release": "run-s test:ci clean build:prod",
    "lint": "eslint --fix \"src/**/*.+(js|vue)\" && eslint --fix \"test/**/*.js\"",
    "dev": "webpack-dev-server --inline --hot --quiet --config webpack.config.js",
    "testem": "testem",
    "testem:ci": "testem ci --launch Firefox",
    "test": "run-p watch:test testem",
    "test:ci": "run-s lint build:test testem:ci"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-unassert": "^2.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "eslint-plugin-html": "^1.5.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^2.3.0",
    "postcss-loader": "^0.9.1",
    "power-assert": "^1.3.1",
    "sinon": "^2.0.0-pre.2",
    "style-loader": "^0.13.1",
    "testem": "^1.6.0",
    "vue-loader": "^9.3.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "k-css": "^1.0.0",
    "vue": "^2.0.0-beta.7",
    "vue-router": "^2.0.0-rc.2",
    "vuex": "^2.0.0-rc.4",
    "vuex-router-sync": "^3.0.0"
  }
}
